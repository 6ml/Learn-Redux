{
    "parser": "babel-eslint",
    "rules": {
         "indent": [2, 2, {"SwitchCase": 1}]
        "no-console" : [0],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "ecmaFeatures" : true
    },
    "plugins": [
        "react"
    ]
}
